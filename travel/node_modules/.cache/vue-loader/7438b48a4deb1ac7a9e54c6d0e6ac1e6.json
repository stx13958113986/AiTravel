ader\">{{list.title}}</div>\r\n\t\t\t\t<div class=\"mui-card-content\">\r\n\t\t\t\t\t<div class=\"mui-card-content-inner\">\r\n                        <p>\r\n                            市场价: <del>￥{{list.p_old}}</del>\r\n                            销售价: <span class=\"now_price\">￥{{list.p_now}}</span>\r\n                        </p>\r\n\t\t\t\t\t<p>\r\n                        购买数量:<div class=\"mui-numbox\">\r\n\t\t\t\t\t<button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"goSub\">-</button>\r\n\t\t\t\t\t<input id=\"test\" class=\"mui-input-numbox\" type=\"number\" value=\"\" v-model=\"num\"/>\r\n\t\t\t\t\t<button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"goAdd\">+</button>\r\n\t\t\t\t</div>\r\n                    </p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"mui-card-footer\">\r\n                    <p>\r\n                        <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n                        <mt-button type=\"danger\" size=\"small\">加入购物车</mt-button>\r\n                    </p>\r\n                </div>\r\n\t\t</div>\r\n             <div class=\"mui-card\">\r\n\t\t\t\t<div class=\"mui-card-header\">商品参数</div>\r\n\t\t\t\t<div class=\"mui-card-content\">\r\n\t\t\t\t\t<div class=\"mui-card-content-inner\">\r\n                        <p>\r\n                      商品货号：{{list.p_sn}}\r\n                        </p>\r\n                        <p>\r\n                    商品类型:{{list.p_type}}\r\n                        </p>\r\n\t\t\t\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"mui-card-footer\">\r\n  \r\n                </div>\r\n\t\t\t</div>\r\n<!-- 商品参数区域 -->\r\n    </div>\r\n</template>\r\n<script>\r\n//导入图片轮播的组件\r\nimport swiper from \"../subcomponents/swiper.vue\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      list: [],\r\n      imagelist: [],\r\n      num: 1\r\n    };\r\n  },\r\n  methods: {\r\n    getGoodsDetail() {\r\n      this.$http.get(\"goodslist/find